<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.eshinetech.rcp.training.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="com.eshinetech.rcp.training.Perspective"
            id="com.eshinetech.rcp.training.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.eshinetech.rcp.training.view.SelectionProviderTestView"
            id="com.eshinetech.rcp.training.view.SelectionProviderTestView"
            name="name"
            restorable="true">
      </view>
   </extension>

</plugin>
